@prefix acp: <http://www.w3.org/ns/solid/acp#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix acr: <https://example.org/test.acl#> .

acr:policy1 a acp:Policy;
    acp:allOf acr:rule1 ;
    acp:allow acp:Read, acp:Write .

acr:accessPolicy1 a acp:Policy;
    acp:allOf acr:rule1 ;
    acp:allow acp:Read, acp:Write .

acr:policy2 a acp:Policy;
    acp:allOf acr:rule2 ;
    acp:allow acp:Read, acp:Write .

acr:accessPolicy2 a acp:Policy;
    acp:allOf acr:rule2 ;
    acp:allow acp:Read, acp:Write .

acr:policy3 a acp:Policy;
    acp:allOf acr:rule3 ;
    acp:allow acp:Read .

acr:policy4 a acp:Policy;
    acp:allOf acr:rule4 ;
    acp:allow acp:Write .

acr:policy5 a acp:Policy;
    acp:allOf acr:rule5 ;
    acp:allow acp:Append .

acr:rule1 a acp:Rule;
    acp:agent <https://example.org/alice#me> .
acr:rule2 a acp:Rule;
    acp:agent <https://example.org/alice#me> .

acr:rule3 a acp:Rule;
        acp:group [
            a vcard:Group;
            vcard:hasMember <https://example.org/alice#me>, <https://example.org/bob#me>
          ] .
acr:rule4 a acp:Rule;
    acp:agent acp:PublicAgent .
acr:rule5 a acp:Rule;
    acp:agent acp:AuthenticatedAgent .

<https://example.org/test.acl>
    acp:apply acr:policy1, acr:policy3, acr:policy4, acr:policy5;
    acp:access acr:accessPolicy1;
    acp:applyMembers acr:policy2;
    acp:accessMembers acr:accessPolicy2
.