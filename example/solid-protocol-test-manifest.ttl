@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix td: <http://www.w3.org/2006/03/test-description#> .

@prefix manifest: <https://raw.githubusercontent.com/solid/conformance-test-harness/main/example/solid-protocol-test-manifest.ttl> .

manifest:content-negotiation
  a td:SpecificationTestCase ;
  dcterms:title "Content Negotiation"@en ;
  dcterms:description "The ability of a server to support different content types in response to the Accept header"@en ;
  td:specificationReference <https://solidproject.org/TR/protocol#resource-representations> ;
  dcterms:hasPart
    <https://github.com/solid/conformance-test-harness/example/content-negotiation/content-negotiation-jsonld.feature>,
    <https://github.com/solid/conformance-test-harness/example/content-negotiation/content-negotiation-turtle.feature>,
    <https://github.com/solid/conformance-test-harness/example/content-negotiation/content-negotiation-rdfa.feature>
.

<https://github.com/solid/conformance-test-harness/example/content-negotiation/content-negotiation-jsonld.feature>
  a td:TestCase ;
  dcterms:subject "MUST" ;
  td:reviewStatus td:accepted ;
  dcterms:title "For a JSON-LD resource" .

<https://github.com/solid/conformance-test-harness/example/content-negotiation/content-negotiation-turtle.feature>
  a td:TestCase ;
  dcterms:subject "MUST" ;
  td:reviewStatus td:accepted ;
  dcterms:title "For a Turtle resource" .

<https://github.com/solid/conformance-test-harness/example/content-negotiation/content-negotiation-rdfa.feature>
  a td:TestCase ;
  dcterms:subject "MAY" ;
  td:reviewStatus td:unreviewed ;
  dcterms:title "For a HTML+RDFa resource" .

manifest:writing-resource
  a td:SpecificationTestCase ;
  dcterms:title "Writing to a resource"@en ;
  dcterms:description "The ability create and amend resources"@en ;
  td:specificationReference <https://solidproject.org/TR/protocol#writing-resources> ;
  dcterms:hasPart
    <https://github.com/solid/conformance-test-harness/example/writing-resource/containment.feature>
.

<https://github.com/solid/conformance-test-harness/example/writing-resource/containment.feature>
  a td:TestCase ;
  dcterms:subject "MUST" ;
  td:reviewStatus td:accepted ;
  dcterms:title "Creating a resource using PUT and PATCH must create intermediate containers" .

manifest:wac-allow
  a td:SpecificationTestCase ;
  dcterms:title "WAC-Allow header"@en ;
  dcterms:description "The ability of a server to show the access allowed for the user and for public users"@en ;
  td:specificationReference <https://solidproject.org/TR/protocol#web-access-control> ;
  td:preCondition "authentication", "acl", "wac-allow" ;
  dcterms:hasPart
    <https://github.com/solid/conformance-test-harness/example/wac-allow/access-Bob-W-public-RA.feature>,
    <https://github.com/solid/conformance-test-harness/example/wac-allow/default-Bob-W-public-RA.feature>,
    <https://github.com/solid/conformance-test-harness/example/wac-allow/access-public-R.feature>
.

<https://github.com/solid/conformance-test-harness/example/wac-allow/access-Bob-W-public-RA.feature>
  a td:TestCase ;
  dcterms:subject "MUST" ;
  td:reviewStatus td:accepted ;
  dcterms:title "Different access for user and public based on accedssTo ACL" .

<https://github.com/solid/conformance-test-harness/example/wac-allow/default-Bob-W-public-RA.feature>
  a td:TestCase ;
  dcterms:subject "MUST" ;
  td:reviewStatus td:accepted ;
  dcterms:title "Different access for user and public based on default ACL" .

<https://github.com/solid/conformance-test-harness/example/wac-allow/access-public-R.feature>
  a td:TestCase ;
  dcterms:subject "MUST" ;
  td:reviewStatus td:unreviewed ;
  dcterms:title "Same access for user and public based on accedssTo ACL" .


